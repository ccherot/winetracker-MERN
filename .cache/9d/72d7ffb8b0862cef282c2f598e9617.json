"source":"SearchParams.js","original":{"line":69,"column":15}},{"generated":{"line":121,"column":8},"source":"SearchParams.js","original":{"line":69,"column":19}},{"name":"pets","generated":{"line":121,"column":10},"source":"SearchParams.js","original":{"line":69,"column":21}},{"generated":{"line":122,"column":0},"source":"SearchParams.js","original":{"line":69,"column":6}},{"generated":{"line":122,"column":4},"source":"SearchParams.js","original":{"line":38,"column":4}},{"generated":{"line":122,"column":5},"source":"SearchParams.js","original":{"line":37,"column":2}},{"generated":{"line":123,"column":0},"source":"SearchParams.js","original":{"line":72,"column":1}},{"generated":{"line":123,"column":1},"source":"SearchParams.js","original":{"line":7,"column":0}},{"name":"SearchParams","generated":{"line":125,"column":15},"source":"SearchParams.js","original":{"line":74,"column":15}}],"sources":{"SearchParams.js":"import React, { useState, useEffect, useContext } from \"react\";\nimport useDropdown from \"./useDropdown\";\nimport pet, { ANIMALS } from \"@frontendmasters/pet\";\nimport Results from \"./Results\";\nimport ThemeContext from \"./ThemeContext\";\n\nconst SearchParams = () => {\n  const [theme, setTheme] = useContext(ThemeContext);\n  const [location, updateLocation] = useState(\"Seattle, WA\");\n  const [breeds, updateBreeds] = useState([]);\n  const [animal, AnimalDropdown] = useDropdown(\"Animal\", \"dog\", ANIMALS);\n  const [breed, BreedDropdown, updateBreed] = useDropdown(\"Breed\", \"\", breeds);\n  const [pets, setPets] = useState([]);\n\n  // get a list of pets\n  async function requestPets() {\n    console.log(\"request pets called\");\n    const { animals } = await pet.animals({\n      location,\n      breed,\n      type: animal\n    });\n\n    console.log(\"animals\", animals);\n    setPets(animals || []);\n  }\n\n  useEffect(() => {\n    updateBreeds([]);\n    updateBreed(\"\");\n    pet.breeds(animal).then(({ breeds }) => {\n      const breedStrings = breeds.map(({ name }) => name);\n      updateBreeds(breedStrings);\n    }, console.error);\n  }, [animal]);\n\n  return (\n    <div className=\"search-params\">\n      <form\n        onSubmit={e => {\n          e.preventDefault();\n          requestPets();\n        }}\n      >\n        <label htmlFor=\"location\">\n          Theme\n          <select\n            value={theme}\n            onChange={e => setTheme(e.target.value)}\n            onBlur={e => setTheme(e.target.value)}\n          >\n            <option value=\"peru\">Peru</option>\n            <option value=\"darkBlue\">Dark Blue</option>\n            <option value=\"chartreuse\">Chartreuse</option>\n            <option mediumorchid=\"Medium Orchid\">Peru</option>\n          </select>\n          Location\n          <input\n            id=\"location\"\n            value={location}\n            placeholder=\"Location\"\n            onChange={e => updateLocation(e.target.value)}\n          />\n        </label>\n        <AnimalDropdown />\n        <BreedDropdown />\n        <button style={{ background: theme }}>Submit</button>\n      </form>\n      <Results pets={pets} />\n    </div>\n  );\n};\n\nexport default SearchParams;\n"},"lineCount":null}},"error":null,"hash":"97aa1fe184566d3200066968b46544c6","cacheData":{"env":{}}}