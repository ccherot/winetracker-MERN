"original":{"line":93,"column":15}},{"generated":{"line":118,"column":37},"source":"Details.js","original":{"line":93,"column":52}},{"generated":{"line":119,"column":0},"source":"Details.js","original":{"line":94,"column":2}},{"generated":{"line":119,"column":9},"source":"Details.js","original":{"line":95,"column":4}},{"generated":{"line":119,"column":38},"source":"Details.js","original":{"line":95,"column":5}},{"generated":{"line":119,"column":60},"source":"Details.js","original":{"line":95,"column":4}},{"generated":{"line":119,"column":68},"source":"Details.js","original":{"line":96,"column":6}},{"generated":{"line":119,"column":97},"source":"Details.js","original":{"line":96,"column":7}},{"generated":{"line":119,"column":104},"source":"Details.js","original":{"line":96,"column":6}},{"name":"props","generated":{"line":119,"column":106},"source":"Details.js","original":{"line":96,"column":19}},{"generated":{"line":119,"column":111},"source":"Details.js","original":{"line":96,"column":6}},{"generated":{"line":119,"column":112},"source":"Details.js","original":{"line":95,"column":4}},{"generated":{"line":119,"column":113},"source":"Details.js","original":{"line":94,"column":2}},{"generated":{"line":120,"column":0},"source":"Details.js","original":{"line":99,"column":1}}],"sources":{"Details.js":"import React from \"react\";\nimport pet from \"@frontendmasters/pet\";\nimport Carousel from \"./Carousel\";\nimport ErrorBoundary from \"./ErrorBoundary\";\nimport ThemeContext from \"./ThemeContext\";\nimport { navigate } from \"@reach/router\";\nimport Modal from \"./Modal\";\n\n// Details is now a Class component.  Class components\n// can still do things that functional compomnents can't\n// but the future still seems to be moving in the direction of\n// functional components\nclass Details extends React.Component {\n  state = { loading: true, showModal: false, error: false };\n\n  // life cycle method that we might otherwise use effects for\n  componentDidMount() {\n    // from the pet API from FrontendMasters\n    pet\n      .animal(this.props.id)\n      .then(({ animal }) => {\n        console.log(\"animal is \", animal);\n        this.setState({\n          name: animal.name,\n          animal: animal.type,\n          location: `${animal.contact.address.city}, ${animal.contact.address.state}`,\n          description: animal.description,\n          media: animal.photos,\n          breed: animal.breeds.primary,\n          loading: false,\n          url: animal.url\n        });\n      })\n      .catch(err => {\n        console.log(\"There was an error loading this listing\", err);\n        this.setState({ error: true });\n        // ErrorBoundary.getDerivedStateFromError(true);\n      });\n  }\n\n  toggleModal = () => this.setState({ showModal: !this.state.showModal });\n  adopt = () => navigate(this.state.url);\n  render() {\n    if (this.state.loading) {\n      console.log(\"state is still loading\");\n      return <h1>loading ...</h1>;\n    }\n\n    const {\n      animal,\n      breed,\n      location,\n      description,\n      name,\n      media,\n      showModal\n    } = this.state;\n\n    return (\n      <div className=\"details\">\n        <Carousel media={media} />\n        <div>\n          <h1>{name}</h1>\n          <h2>\n            {animal} - {breed} - {location}\n          </h2>\n          <ThemeContext.Consumer>\n            {([theme]) => (\n              <button\n                style={{ backgroundColor: theme }}\n                onClick={this.toggleModal}\n              >\n                Adopt {name}\n              </button>\n            )}\n          </ThemeContext.Consumer>\n          <p>{description}</p>\n          {showModal ? (\n            <Modal>\n              <h1>Would you like to adopt {name}?</h1>\n              <div className=\"buttons\">\n                <button onClick={this.adopt}>Yes</button>\n                <button onClick={this.toggleModal}>No</button>\n              </div>\n            </Modal>\n          ) : null}\n        </div>\n      </div>\n    );\n  }\n}\n\nexport default function DetailsErrorBoundary(props) {\n  return (\n    <ErrorBoundary>\n      <Details {...props} />\n    </ErrorBoundary>\n  );\n}\n"},"lineCount":null}},"error":null,"hash":"cd29622620c6bbd2149de59d9b59d47e","cacheData":{"env":{}}}